      "predictiveBackGestureEnabled": false,
      "package": "com.hk2008.where"
    },
    "web": {
      "favicon": "./src/assets/images/logo.png"
    },
    "fonts": {
      "Pretendard-Variable": "./src/assets/fonts/pretendard/PretendardVarable.ttf"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "29078c32-6c0d-4ab2-ab55-2b2f4cadb874"
      }
    },
    "owner": "hk2008"
